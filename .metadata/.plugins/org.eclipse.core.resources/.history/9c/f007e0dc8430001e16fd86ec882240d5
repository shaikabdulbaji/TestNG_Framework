package b32MultipleDataProvidermethods;

import org.testng.annotations.DataProvider;
import org.testng.annotations.Test;

public class aDemo {
	
	@Test(dataProvider = "supplierOne")
	public void sample(String one, String two, String three)
	{
		System.out.println(one+" _ "+two+" _ "+three);
	}

	@DataProvider(name="supplierOne")
	public String[][] dataSupplier1()
	{
		String[][] data = {{"Abdul","Baji","Mango"},
				{"Shaik","Rasool","Apple"},
				{"Kareem","Amma","banana"}};
		
		return data;	
	}
	
	// This session about "Multiple DataProvider methods"
	
	@DataProvider(name="supplierTwo")
	public String[][] dataSupplier2()
	{
		String[][] data = {{"Shaik","Baji","Red"},
				{"Abdul","Rasool","Black"},
				{"Kareem","Shaik","White"}};
		
		return data;	
	}
}
