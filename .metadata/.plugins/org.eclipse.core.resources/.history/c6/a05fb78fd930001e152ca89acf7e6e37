package b37PartiallypassingthedatafromDataProvidermethodusingindicesattribute;

import org.testng.annotations.DataProvider;
import org.testng.annotations.Test;

public class aDemo {
	
	@Test(dataProvider = "supplier")
	public void sample(Object[] a)
	{
		
		System.out.println(a);
		
	}
	

	@DataProvider(name="supplier")
	public Object[][] dataSupplier()
	{
		Object[][] data = {{"one","two","three", "four","five","six","seven","eight","nine","ten"}};
		
		return data;	
	}

}
